package arrayexpansion;

public class ArrayExpansion {

    
    public static void main(String[] args) {
        int[]arr = {2, 3, 4, 5, 6, 9, 8, 7, 1, 8};
        int[]original = arr;
        outputArray(arr);
        System.out.println();
        outputArray(arr);
        System.out.println();
        outputArray(original);
        System.out.println();
        arr = new int[12];
        outputArray(arr);
        System.out.println();
        outputArray(original);
        System.out.println();
        for(int i=0;i<original.length;i++)
            arr[i]=original[i];
        outputArray(arr);
        System.out.println();
        outputArray(original);
        System.out.println();
        outputArray(arr);
      System.out.println();
      if(arr == original)
          outputArray(original);
      else
          System.out.println("Null");
       
        
    }
     public static void outputArray( int[] array)
 {
 // loop through array's rows
 for ( int row = 0; row < array.length; row++ )
 {
System.out.printf( "%d ", array[ row ] );

 
}
 }

}
